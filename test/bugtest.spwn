let PI          = 314159.2653589793;
let HALF_PI     = 157079.6326794897;
let DOUBLE_PI   = 628318.5307179586;
let SIN_CURVE_A = 004158.96;
let SIN_CURVE_B = 000129.810625032;

let x = counter(?i)
x.display(100,200)
let r = counter(?i)
r.display(100,200)
let cos1 = !{
    if x < 0 {
        let q = x*-1 / DOUBLE_PI;
        q += 1;
        let y = q * DOUBLE_PI;
        x = (x - y)*-1;
    }
    if x >= DOUBLE_PI {
        let q = x / DOUBLE_PI;
        let y = q * DOUBLE_PI;
        x = x - y;
    }
    let s = counter(1);
    if x >= PI {
        s = -1;
        x -= PI;
    }
    if x > HALF_PI {
        x = counter(PI) - x;
        s = s*-1;
    }
    let z = x * x;
    r = z * (z * (counter(SIN_CURVE_A) - z*SIN_CURVE_B) - 0.5) + 1.0;
    if r > 1.0 {
        r = r - 2.0;
    } 
    if s > 0 {
        r = r*-1;
    }
}

x = 40
cos1!